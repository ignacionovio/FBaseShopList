{
  "expo": {
    "name": "GastosApp",
    "slug": "rnfbase",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./payment-method.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "App para control personal de gastos",
    "android": {
      "package": "com.inovio.rnfbase",
      "useNextNotificationsApi": true,
      "permissions": [
        "RECEIVE_BOOT_COMPLETED"
      ]
    }
  }
}
